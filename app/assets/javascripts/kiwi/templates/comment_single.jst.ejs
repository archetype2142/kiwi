<div class="up-vote">
  <i class="fa fa-sort-up"></i>
  <i class="fa fa-sort-down"></i>
</div>
<div class="user-comment">
  <div class="info">
    <span class="username"> <%= username %> </span>
    <div class="tools">
      <span class="upvotes"><%= upvotes %></span> upvote(s) - <span class="reply">Reply</span>- Report <% if (canDelete) { %>- <span class="delete">Delete</span><% } %></div>
  </div>
  <div class="comment-text"><%= message() %></div>
</div>
<div class="clearfix"></div>
<div class="nested-comments">
  <div class="replies-region"></div>
  <div class="replybox-region"></div>
</div>
<div class="clearfix"></div>
