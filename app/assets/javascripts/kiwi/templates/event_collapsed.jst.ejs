<div class='upvote-container pull-left'>
  <i class='icon-caret-up'></i>
  <div class='upvote-counter'></div>
</div>
<div class='image-container pull-left'>
  <img src='<%=thumbUrl%>' />
</div>
<div class='meta-container pull-left'>
  <a class='event-name'><%=name%> - <%=time()%><% if (location_type == 'international') { %> - Global <% } %> </a><br />
  <span class='event-meta'>Added by <%=user%> - <%=fullSubkastName()%></span>
</div>
<div class='clearfix'></div>
